@page "/notfound"

<h1>Page Not Found</h1>
<p>Sorry, the page you are looking for does not exist.</p>
<button @onclick="GoHome">Go to Home</button>

@code {
    [Inject]
    private NavigationManager Navigation { get; set; } = default!;

    private void GoHome()
    {
        Navigation.NavigateTo("/");
    }
}